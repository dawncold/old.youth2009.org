</span><span class='line'>&lt;h1>! /usr/bin/env python&lt;/h1>
</span><span class='line'>
</span><span class='line'>&lt;h1>coding: utf-8&lt;/h1>
</span><span class='line'>
</span><span class='line'>&lt;p>import sqlite3&lt;/p>
</span><span class='line'>
</span><span class='line'>&lt;p>import os&lt;/p>
</span><span class='line'>
</span><span class='line'>&lt;p>import time&lt;/p>
</span><span class='line'>
</span><span class='line'>&lt;p>import random&lt;/p>
</span><span class='line'>
</span><span class='line'>&lt;p>def connect():&lt;/p>
</span><span class='line'>
</span><span class='line'>&lt;p>return sqlite3.connect(&lsquo;./test.db&rsquo;)&lt;/p>
</span><span class='line'>
</span><span class='line'>&lt;p>def insertProgress(conn, user_id):&lt;/p>
</span><span class='line'>
</span><span class='line'>&lt;p>cur = conn.execute(&lsquo;INSERT INTO todo(title, date) VALUES(?,?)&rsquo;, [&lsquo;just test!!!&rsquo;, time.strftime(&ldquo;%Y-%m-%d %H:%M:%S&rdquo;, time.localtime())])&lt;/p>
</span><span class='line'>
</span><span class='line'>&lt;p>conn.execute(&lsquo;INSERT INTO todo_meta(todo_id, user_id) VALUES(?,?)&rsquo;, [cur.lastrowid, user_id])&lt;/p>
</span><span class='line'>
</span><span class='line'>&lt;p>def insertManyData(conn):&lt;/p>
</span><span class='line'>
</span><span class='line'>&lt;h1>reset database&lt;/h1>
</span><span class='line'>
</span><span class='line'>&lt;p>os.system(&lsquo;sqlite3 test.db &lt; schema.sql&rsquo;)&lt;/p>
</span><span class='line'>
</span><span class='line'>&lt;p>conn.cursor()&lt;/p>
</span><span class='line'>
</span><span class='line'>&lt;p>counts = [0,0,0,0,0,0]&lt;/p>
</span><span class='line'>
</span><span class='line'>&lt;p>for i in xrange(100000):&lt;/p>
</span><span class='line'>
</span><span class='line'>&lt;p>userid = random.choice([0, 1, 2, 3, 4, 5])&lt;/p>
</span><span class='line'>
</span><span class='line'>&lt;p>counts[userid] += 1&lt;/p>
</span><span class='line'>
</span><span class='line'>&lt;p>insertProgress(conn, userid)&lt;/p>
</span><span class='line'>
</span><span class='line'>&lt;p>conn.commit()&lt;/p>
</span><span class='line'>
</span><span class='line'>&lt;p>conn.close()&lt;/p>
</span><span class='line'>
</span><span class='line'>&lt;p>print counts&lt;/p>
</span><span class='line'>
</span><span class='line'>&lt;p>def selectUsingLeftOuterJoin(conn, user_id):&lt;/p>
</span><span class='line'>
</span><span class='line'>&lt;p>conn.cursor()&lt;/p>
</span><span class='line'>
</span><span class='line'>&lt;p>cur = conn.execute(&lsquo;SELECT t.title, t.date \&lt;/p>
</span><span class='line'>
</span><span class='line'>&lt;p>FROM todo t LEFT OUTER JOIN todo_meta tm \&lt;/p>
</span><span class='line'>
</span><span class='line'>&lt;p>ON tm.todo_id = t.id\WHERE tm.user_id = ?&lsquo;, [user_id])&lt;/p>
</span><span class='line'>
</span><span class='line'>&lt;p>def selectTwoTimes(conn, user_id):&lt;/p>
</span><span class='line'>
</span><span class='line'>&lt;p>conn.cursor()&lt;/p>
</span><span class='line'>
</span><span class='line'>&lt;p>cur = conn.execute(&lsquo;SELECT todo_id FROM todo_meta WHERE user_id = ?&rsquo;, [user_id])&lt;/p>
</span><span class='line'>
</span><span class='line'>&lt;p>for tid in [row[0] for row in cur.fetchall()]:&lt;/p>
</span><span class='line'>
</span><span class='line'>&lt;p>cur = conn.execute(&lsquo;SELECT title, date FROM todo WHERE id = ?&rsquo;, [tid])&lt;/p>
</span><span class='line'>
</span><span class='line'>&lt;p>if &lt;strong>name&lt;/strong> == &ldquo;&lt;strong>main&lt;/strong>&rdquo;:&lt;/p>
</span><span class='line'>
</span><span class='line'>&lt;p>uids = [0, 1, 2, 3, 4, 5]&lt;/p>
</span><span class='line'>
</span><span class='line'>&lt;p>insert_time_start = time.time()&lt;/p>
</span><span class='line'>
</span><span class='line'>&lt;p>insertManyData(connect())&lt;/p>
</span><span class='line'>
</span><span class='line'>&lt;p>print &ldquo;Insert Time: %f&rdquo; % (time.time() &ndash; insert_time_start)&lt;/p>
</span><span class='line'>
</span><span class='line'>&lt;p>select_ULOJ_start = time.time()&lt;/p>
</span><span class='line'>
</span><span class='line'>&lt;p>for uid in uids:&lt;/p>
</span><span class='line'>
</span><span class='line'>&lt;p>selectUsingLeftOuterJoin(connect(), uid)&lt;/p>
</span><span class='line'>
</span><span class='line'>&lt;p>print &ldquo;Select Using Left Outer Join time: %f&rdquo; % (time.time() &ndash; select_ULOJ_start)&lt;/p>
</span><span class='line'>
</span><span class='line'>&lt;p>select_two_times = time.time()&lt;/p>
</span><span class='line'>
</span><span class='line'>&lt;p>for uid in uids:&lt;/p>
</span><span class='line'>
</span><span class='line'>&lt;p>selectTwoTimes(connect(), uid)&lt;/p>
</span><span class='line'>
</span><span class='line'>&lt;p>print &ldquo;Select Two Times time: %f&rdquo; % (time.time() &ndash; select_two_times)&lt;/p>
</span><span class='line'>
</span><span class='line'>&lt;p></span></code></pre></td></tr></table></div></figure></p>

